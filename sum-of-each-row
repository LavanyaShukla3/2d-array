class HelloWorld {
    public static void main(String[] args) {
        
        int[][] a = {{9,8,7},
                    {6,5,4},
                    {3,2,1},
                    {10,11,12}};
        
        //int [] sum = new int[4][1]             
        for(int i = 0; i< a.length; i++){
            int sum = 0;
            for(int j = 0; j< a[0].length; j++){
                sum += a[i][j];
    
            }
            System.out.println(sum);
        }
   }
}
